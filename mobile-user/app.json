{
  "expo": {
    "name": "App Urbana Reporta",
    "slug": "urbana-reporta-user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/usuario.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": ".app/assets/usuario.png",
      "resizeMode": "contain",
      "backgroundColor": "#16a34a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.urbanareporta.user"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#16a34a"
      },
      "package": "com.urbanareporta.user",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permitir que $(PRODUCT_NAME) use tu ubicación para reportes."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir que $(PRODUCT_NAME) acceda a tu cámara para tomar fotos de reportes."
        }
      ]
    ],
    "scheme": "urbanareporta"
  }
}
